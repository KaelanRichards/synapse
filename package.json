{
  "name": "synapse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:local": "supabase start && next dev",
    "dev:prod": "NODE_ENV=production next dev",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:diff": "supabase db diff -f migration",
    "db:generate": "supabase migration new",
    "db:seed": "supabase db reset && psql -h localhost -p 54322 -U postgres -d postgres -f supabase/seed.sql",
    "db:seed:prod": "psql \"$DATABASE_URL\" -f supabase/seed.sql",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/supabase-js": "^2.39.2",
    "@types/diff": "^7.0.0",
    "@types/lodash": "^4.17.14",
    "date-fns": "^4.1.0",
    "diff": "^7.0.0",
    "lodash": "^4.17.21",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "react-hook-form": "^7.49.2",
    "reactflow": "^11.11.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
