    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.33",
    "supabase": "latest",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
